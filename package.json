{
    "name": "cache-in-storage",
    "version": "1.0.0",
    "description": "前端缓存工具",
    "main": "dist/index.min.js",
    "typings": "dist/index.d.ts",
    "types": "dist/index.d.ts",
    "scripts": {
        "clear": "rm -rf dist;rm -rf libs;rm -rf coverage;",
        "prod": "NODE_ENV=production webpack --config=webpack/webpack.prod.config.js",
        "libs": "tsc -d --outDir libs",
        "build": "npm run clear;npm run prod;npm run libs",
        "test": "mocha ./tests/**/*.spec.js --require @babel/register",
        "karma": "karma start karma.conf.js",
        "cover": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha ./tests/**/*.spec.js -- --require @babel/register",
        "coveralls": "npm run cover; cat ./coverage/lcov.info | coveralls"
    },
    "keywords": [
        "cache",
        "frontend",
        "promise"
    ],
    "author": "NICK <nick121212@126.com>",
    "license": "MIT",
    "nyc": {
        "require": [
            "@babel/register"
        ],
        "reporter": [
            "lcov",
            "text"
        ],
        "sourceMap": false,
        "instrument": false
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.4.3",
        "@babel/node": "^7.5.5",
        "@babel/plugin-transform-runtime": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/register": "^7.5.5",
        "@babel/runtime": "^7.5.5",
        "@types/node": "^11.13.6",
        "babel-istanbul": "^0.12.2",
        "babel-loader": "^8.0.5",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "coveralls": "^3.0.6",
        "istanbul": "^1.0.0-alpha.2",
        "istanbul-instrumenter-loader": "^3.0.1",
        "karma": "^4.1.0",
        "karma-chai": "^0.1.0",
        "karma-chai-as-promised": "^0.1.2",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.2",
        "karma-coverage-istanbul-reporter": "^2.0.2",
        "karma-mocha": "^1.3.0",
        "karma-mocha-reporter": "^2.2.5",
        "karma-spec-reporter": "^0.0.32",
        "karma-webpack": "^3.0.5",
        "mocha": "^6.1.4",
        "mochawesome": "^3.1.2",
        "nyc": "^14.1.1",
        "ts-loader": "^5.4.3",
        "typescript": "^3.5.3",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.7"
    }
}